<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="389.1dp"
        android:layout_height="668.0dp"
        android:layout_centerVertical="true"
        android:layout_margin="10dp"
        android:orientation="vertical"
        android:layout_marginTop="0.0dp"
        android:layout_marginBottom="20.0dp">

        <ImageView
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="132.9dp"
            android:src="@drawable/ctaicon"
            android:id="@+id/imageView1" />

        <TextView
            android:layout_marginTop="10dp"
            android:text="CONTACT TRACING APPLICATION"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minWidth="25px"
            android:minHeight="25px"
            android:gravity="center"
            android:textColor="#0070b9"
            android:textStyle="bold"
            android:textSize="22dp"
            android:id="@+id/textViewLOG" />

        <Button
            android:layout_marginTop="10dp"
            android:text="COVID - 19 INFORMATION"
            android:layout_width="294.3dp"
            android:layout_height="90.0dp"
            android:minWidth="800px"
            android:minHeight="300px"
            android:layout_gravity="center"
            android:background="#0070b9"
            android:textColor="@android:color/white"
            android:textStyle="bold"
            android:textSize="22dp"
            android:id="@+id/covidinfo" />

        <Button
            android:layout_marginTop="10dp"
            android:text="HEALTH DECLARATION FORM"
            android:layout_width="294.3dp"
            android:layout_height="85.7dp"
            android:minWidth="800px"
            android:minHeight="300px"
            android:layout_gravity="center"
            android:background="#0070b9"
            android:textColor="@android:color/white"
            android:textStyle="bold"
            android:textSize="22dp"
            android:id="@+id/healthform" />

        <Button
            android:layout_marginTop="10dp"
            android:text="QR CODE"
            android:layout_width="294.3dp"
            android:layout_height="90.0dp"
            android:minWidth="800px"
            android:minHeight="300px"
            android:layout_gravity="center"
            android:background="#0070b9"
            android:textColor="@android:color/white"
            android:textStyle="bold"
            android:textSize="25dp"
            android:id="@+id/viewqr" />

        <Button
            android:layout_marginTop="10dp"
            android:text="LOGOUT"
            android:layout_width="294.3dp"
            android:layout_height="87.7dp"
            android:minWidth="800px"
            android:minHeight="300px"
            android:layout_gravity="center"
            android:background="#0070b9"
            android:textColor="@android:color/white"
            android:textStyle="bold"
            android:textSize="25dp"
            android:id="@+id/logout" />
    </LinearLayout>
</RelativeLayout>
